<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="teamh.zapapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HompageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".FingerprintAuthActivity" />
        <activity android:name=".VoicednaAuthActivity" />
        <activity android:name=".MagiclinkAuthActivity">
            <!-- Auto generated stub. Tutorial at: https://g.co/AppIndexing/AndroidStudio  -->
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Auto generated stub. HTTP scheme is recommended.
                  TODO: Change the host or pathPrefix as necessary. -->
                <data
                    android:host="www.zapapp.com"
                    android:pathPrefix="/magiclinkauth"
                    android:scheme="http" />
            </intent-filter>
        </activity>
    </application>
    <uses-feature android:name="android.hardware.fingerprint"
        android:required="false"/>
    <uses-permission
        android:name="android.permission.USE_FINGERPRINT" />

</manifest>