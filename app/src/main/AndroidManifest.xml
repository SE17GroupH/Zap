<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="teamh.zapapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HompageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".FingerprintAuthActivity" />
        <activity android:name=".VoicednaAuthActivity" />
        <activity android:name=".MagiclinkAuthActivity"/>
        <activity android:name=".LoginFail"/>
        <activity android:name=".LoginSuccess"/>

    </application>
    <uses-feature android:name="android.hardware.fingerprint"
        android:required="false"/>
    <uses-permission
        android:name="android.permission.USE_FINGERPRINT" />
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>